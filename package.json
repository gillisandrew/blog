{
    "name": "gatsby-starter-netlify-cms",
    "description": "Example Gatsby, and Netlify CMS project",
    "version": "1.1.3",
    "author": "Austin Green",
    "dependencies": {
        "bulma": "^0.7.0",
        "gatsby": "^2.1.34",
        "gatsby-image": "^2.0.23",
        "gatsby-plugin-manifest": "^2.1.1",
        "gatsby-plugin-netlify": "^2.0.6",
        "gatsby-plugin-netlify-cms": "^4.0.0",
        "gatsby-plugin-postcss": "^2.0.7",
        "gatsby-plugin-purgecss": "^3.1.0",
        "gatsby-plugin-react-helmet": "^3.0.4",
        "gatsby-plugin-sass": "^2.0.7",
        "gatsby-plugin-sharp": "^2.0.15",
        "gatsby-plugin-typescript": "^2.0.15",
        "gatsby-plugin-web-font-loader": "^1.0.4",
        "gatsby-remark-copy-linked-files": "^2.0.7",
        "gatsby-remark-images": "^3.0.1",
        "gatsby-remark-relative-images": "^0.2.1",
        "gatsby-source-filesystem": "^2.0.26",
        "gatsby-transformer-remark": "^2.3.4",
        "gatsby-transformer-sharp": "^2.1.9",
        "lodash": "^4.17.5",
        "lodash-webpack-plugin": "^0.11.4",
        "netlify-cms-app": "^2.9.1",
        "netlify-cms-media-library-cloudinary": "^1.3.2",
        "netlify-cms-media-library-uploadcare": "^0.5.2",
        "node-sass": "^4.11.0",
        "parcel-bundler": "^1.9.4",
        "prop-types": "^15.6.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-helmet": "^5.2.0",
        "react-zondicons": "^1.1.9",
        "typescript": "^3.5.1",
        "uuid": "^3.2.1"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "main": "n/a",
    "scripts": {
        "clean": "rimraf .cache public",
        "start:app": "npm run develop",
        "start:lambda": "netlify-lambda serve src/lambda",
        "start": "run-p start:**",
        "build:app": "npm run clean && node -r esm ./node_modules/.bin/gatsby build",
        "build:lambda": "netlify-lambda build src/lambda",
        "build": "run-p build:**",
        "develop": "npm run clean && node -r esm ./node_modules/.bin/gatsby develop",
        "serve": "node -r esm ./node_modules/.bin/gatsby serve",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "graphql-types": "graphql-codegen",
        "fix:eslint": "prettier-eslint --write 'src/**/*.{js,jsx,ts,tsx}'",
        "fix:stylelint": "prettier-stylelint --write 'src/**/*.{css,scss}'"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.2.0",
        "@graphql-codegen/typescript": "^1.2.0",
        "@graphql-codegen/typescript-operations": "^1.2.0",
        "@types/graphql": "^14.2.0",
        "@types/lodash": "^4.14.132",
        "@types/node": "^12.0.3",
        "@types/react": "^16.8.4",
        "@types/react-dom": "^16.8.4",
        "@types/react-helmet": "^5.0.8",
        "@types/sharp": "^0.22.2",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "esm": "^3.2.25",
        "http-proxy-middleware": "^0.19.1",
        "netlify-lambda": "^1.4.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.15.3",
        "prettier-eslint": "^8.8.2",
        "prettier-eslint-cli": "^4.7.1",
        "prettier-stylelint": "^0.4.2",
        "rimraf": "^3.0.0",
        "stylelint": "^10.0.1",
        "stylelint-config-recommended": "^2.2.0",
        "tailwindcss": "^1.0.2",
        "tailwindcss-alpha": "^0.1.4",
        "tailwindcss-aspect-ratio": "^1.0.3"
    }
